# LeetCoach Environment Variables Example
# Copy this file to .env and fill in your actual values

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/LeetCoach
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/LeetCoach

# Server Configuration
PORT=5000
NODE_ENV=development

# AI Service Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Frontend Configuration (if needed)
REACT_APP_API_URL=http://localhost:5000/api

# Session Configuration
SESSION_SECRET=your_session_secret_here

# Optional: JWT Secret (if you add authentication)
JWT_SECRET=your_jwt_secret_here
